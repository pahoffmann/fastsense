<launch>
  #### Automatically set sim time false
  <param name="/use_sim_time" value="false"/>
  <arg name="/save_dir" default="/home/kbs/fastsense/"/>


  #### Bridge 
  <include file="$(find bridge)/launch/bridge.launch"/>

  #### Recorder
  <node pkg="rosbag" type="record" name="fastsense_recorder" output="screen" args="-o $(arg save_dir) --all -x /from_trenz_bridge/tsdf"/>   
</launch>
